<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean id="transactionSample" parent="baseTransactionProxy">
		<property name="target">
			<bean class="org.anyframe.transaction.TransactionTestSampleServiceImpl">
				<property name="queryService" ref="queryService"></property>
			</bean>
		</property>
		<property name="proxyInterfaces">
			<list>
				<value>org.anyframe.transaction.TransactionTestSampleService</value>
			</list>
		</property>
	</bean>
</beans>